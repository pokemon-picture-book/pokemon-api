{
  "name": "pokemon-api",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "npm run build && node dist/app.js",
    "dev": "npm run build:dev & nodemon ./dist/app.js",
    "build:dev": "npm run clean && webpack --config webpack.dev.js --watch",
    "build": "npm run clean && webpack --config webpack.prod.js",
    "clean": "rimraf dist/*",
    "scan": "sonar-scanner",
    "lint": "eslint --fix --ext .ts .",
    "test": "jest --coverage --verbose",
    "tn": "ts-node",
    "postmerge": "(git diff 'HEAD@{1}' --name-only | grep package.json > /dev/null) && npm ci || :",
    "migrate:generate": "ts-node $(npm bin)/typeorm migration:generate -c development -n",
    "migrate:create": "ts-node $(npm bin)/typeorm migration:create -c development -n",
    "migrate:run": "ts-node $(npm bin)/typeorm migration:run -c $NODE_ENV",
    "migrate:revert": "ts-node $(npm bin)/typeorm migration:revert -c $NODE_ENV"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "0.21.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "copy-webpack-plugin": "5.1.1",
    "express": "^4.17.1",
    "express-validator": "6.6.1",
    "fs": "0.0.1-security",
    "inversify": "^5.0.1",
    "logform": "^2.1.2",
    "moment": "^2.24.0",
    "mysql": "2.18.1",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "supertest": "4.0.2",
    "typeorm": "0.2.29",
    "uuid": "^3.3.2",
    "webpack": "5.10.1",
    "webpack-filter-warnings-plugin": "1.2.1",
    "webpack-merge": "5.7.0",
    "winston": "^3.2.1",
    "yaml": "^1.7.2"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/polyfill": "7.12.1",
    "@babel/preset-env": "7.12.10",
    "@babel/preset-typescript": "7.12.7",
    "@types/axios": "^0.14.0",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "^4.17.1",
    "@types/express-validator": "3.0.0",
    "@types/jest": "26.0.19",
    "@types/js-yaml": "^3.12.1",
    "@types/loglevel": "^1.6.3",
    "@types/moment": "^2.13.0",
    "@types/mysql": "2.15.10",
    "@types/node": "14.14.13",
    "@types/supertest": "2.0.10",
    "@types/uuid": "^3.4.5",
    "@types/winston": "^2.4.4",
    "@types/yaml": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "babel-loader": "8.2.2",
    "eslint": "7.15.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-prettier": "3.2.0",
    "husky": "^4.0.0-beta.2",
    "jest": "26.6.3",
    "lint-staged": "^9.4.0",
    "nodemon": "2.0.6",
    "prettier": "2.2.1",
    "rimraf": "^3.0.0",
    "sonar-scanner": "^3.1.0",
    "ts-jest": "26.4.4",
    "ts-loader": "8.0.12",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.1",
    "typescript": "4.1.3",
    "webpack-cli": "4.2.0"
  },
  "engines": {
    "node": ">= 14.0.0",
    "npm": ">= 6.14.4"
  }
}
