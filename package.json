{
  "name": "pokemon-api",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "dev": "rm -rf dist && webpack --config webpack.dev.js && node dist/app.js",
    "build": "webpack --config webpack.dev.js",
    "clean": "rm -rf dist",
    "scan": "sonar-scanner",
    "lint": "eslint --fix --ext .ts src/",
    "test": "jest",
    "postmerge": "(git diff 'HEAD@{1}' --name-only | grep package.json > /dev/null) && npm ci || :",
    "migrate:generate": "ts-node $(npm bin)/typeorm migration:generate -n",
    "migrate:create": "ts-node $(npm bin)/typeorm migration:create -n",
    "migrate:run": "ts-node $(npm bin)/typeorm migration:run",
    "migrate:revert": "ts-node $(npm bin)/typeorm migration:revert",
    "create-seed": "ts-node db/seed/createSeedData.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "inversify": "^5.0.1",
    "js-yaml": "^3.13.1",
    "logform": "^2.1.2",
    "moment": "^2.24.0",
    "path": "^0.12.7",
    "pg": "^7.12.0",
    "progress": "^2.0.3",
    "puppeteer": "^1.19.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.10-rc.1",
    "uuid": "^3.3.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/js-yaml": "^3.12.1",
    "@types/loglevel": "^1.6.3",
    "@types/moment": "^2.13.0",
    "@types/node": "^12.7.12",
    "@types/pg": "^7.4.14",
    "@types/progress": "^2.0.3",
    "@types/puppeteer": "^1.19.0",
    "@types/uuid": "^3.4.5",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-watch": "^7.0.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^4.0.0-beta.2",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.0",
    "prettier": "^1.18.2",
    "sonar-scanner": "^3.1.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.3.0",
    "typescript": "3.5.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  }
}
