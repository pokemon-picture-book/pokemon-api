{
  "name": "pokemon-api",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "scripts": {
    "start": "NODE_ENV=production node dist/app.js",
    "dev": "ts-node-dev -C ttypescript --files src/app.ts",
    "build": "rimraf dist/* && ttsc",
    "clean": "rimraf dist/*",
    "scan": "sonar-scanner",
    "lint": "eslint --fix --ext .ts .",
    "test": "jest",
    "tn": "ts-node",
    "postmerge": "(git diff 'HEAD@{1}' --name-only | grep package.json > /dev/null) && npm ci || :",
    "migrate:generate": "ts-node $(npm bin)/typeorm migration:generate -n -c development",
    "migrate:create": "ts-node $(npm bin)/typeorm migration:create -n -c development",
    "migrate:run": "ts-node $(npm bin)/typeorm migration:run -c $NODE_ENV",
    "migrate:revert": "ts-node $(npm bin)/typeorm migration:revert -c $NODE_ENV",
    "create-seed": "ts-node db/seed/createSeedData.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "git add"
    ]
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "inversify": "^5.0.1",
    "logform": "^2.1.2",
    "moment": "^2.24.0",
    "path": "^0.12.7",
    "pg": "^7.12.0",
    "progress": "^2.0.3",
    "puppeteer": "^1.20.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.20",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "yaml": "^1.7.2"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.18",
    "@types/js-yaml": "^3.12.1",
    "@types/loglevel": "^1.6.3",
    "@types/moment": "^2.13.0",
    "@types/node": "^12.7.12",
    "@types/pg": "^7.4.14",
    "@types/progress": "^2.0.3",
    "@types/puppeteer": "^1.19.0",
    "@types/uuid": "^3.4.5",
    "@types/winston": "^2.4.4",
    "@types/yaml": "^1.2.0",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "babel-watch": "^7.0.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^4.0.0-beta.2",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "sonar-scanner": "^3.1.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.2.0",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.44",
    "ttypescript": "^1.5.7",
    "typescript": "3.5.3",
    "typescript-transform-paths": "^1.1.11"
  },
  "engines": {
    "node": ">= 11.10.0",
    "npm": ">= 6.7.0"
  }
}
